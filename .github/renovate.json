{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)",
    "github>Turbo87/renovate-config//rust/updateToolchain"
  ],
  "platformAutomerge": true,
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ]
}
